<div class='tcr1 forms col-sm-12'>
  <h4>TCR 1</h4>
  <form class='form'>
    <div class='form-group'>
      <label for='transCode'>Transaction Code</label>
      <select id='transCode' name='transCode' class='form-control'>
        <option value='05'>Sales Draft</option>
        <option value='06'>Credit Voucher</option>
        <option value='07'>Cash Disbursement</option>
        <option value='15'>Sales Draft Chargeback</option>
        <option value='16'>Credit Voucher Chargeback</option>
        <option value='17'>Cash Disbursement Chargeback</option>
      </select>
    </div>
    <div class='form-group'>
      <label for='transCodeQualifier'>Transaction Code Qualifier</label>
      <select id='transCodeQualifier' name='transCodeQualifier' class='form-control'>
        <option value='0'>Default</option>
        <option value='1'>Account Funding</option>
        <option value='2'>Original Credit</option>
      </select>
    </div>
    <input type='hidden' class='form-control' id='transCompSeqNum' name='transCompSeqNum' maxlength='1' value='1'>
    <input type='hidden' class='form-control' id='businessFormatCode' name='businessFormatCode' maxlength='1' value=' '>
    <input type='hidden' class='form-control' id='tokenAssuranceLevel' name='tokenAssuranceLevel' maxlength='1' value=' '>
    <input type='hidden' class='form-control' id='reserved1' name='reserved1' maxlength='9' value='         '>
    <div class='form-group'>
      <label for='chargebackRefNum'>Chargeback Reference Number</label>
      <input type='text' class='form-control' id='chargebackRefNum' name='chargebackRefNum' maxlength='6'>
    </div>
    <div class='form-group'>
      <label for='documentationIndicator'>Documentation Indicator</label>
      <select id='documentationIndicator' name='documentationIndicator' class='form-control'>
        <option value=' '>No support documentation required</option>
        <option value='0'>No support documentation provided</option>
        <option value='1'>Support documentation to follow</option>
        <option value='2'>Invalid ARN and no supporting documentation required</option>
        <option value='3'>Invalid ARN and no supporting documentation received</option>
      </select>
    </div>
    <div class='form-group'>
      <label for='memberMessageText'>Member Message Text</label>
      <input type='text' class='form-control' id='memberMessageText' name='memberMessageText' maxlength='50'>
    </div>
    <div class='form-group'>
      <label for='specialConditionIndicators'>Special Condition Indicators</label>
      <input type='text' class='form-control' id='specialConditionIndicators' name='specialConditionIndicators' maxlength='2'>
    </div>
    <div class='form-group'>
      <label for='feeProgramIndicator'>Fee Program Indicators</label>
      <input type='text' class='form-control' id='feeProgramIndicator' name='feeProgramIndicator' maxlength='3'>
    </div>
    <div class='form-group'>
      <label for='issuerCharge'>Issuer Charge</label>
      <input type='text' class='form-control' id='issuerCharge' name='issuerCharge' maxlength='1'>
    </div>
    <input type='hidden' class='form-control' id='reserved2' name='reserved2' maxlength='1' value=' '>
    <div class='form-group'>
      <label for='cardAcceptorID'>Card Acceptor ID</label>
      <input type='text' class='form-control' id='cardAcceptorID' name='cardAcceptorID' maxlength='15'>
    </div>
    <div class='form-group'>
      <label for='terminalID'>Terminal ID</label>
      <input type='text' class='form-control' id='terminalID' name='terminalID' maxlength='8'>
    </div>
    <div class='form-group'>
      <label for='nationalReimbursementFee'>National Reimbursement Fee</label>
      <input type='text' class='form-control' id='nationalReimbursementFee' name='nationalReimbursementFee' maxlength='12'>
    </div>
    <div class='form-group'>
      <label for='commerceAndPaymentIndicator'>Mail/Phone/Electronic Commerce Payment Indicator</label>
      <input type='text' class='form-control' id='commerceAndPaymentIndicator' name='commerceAndPaymentIndicator' maxlength='1'>
    </div>
    <div class='form-group'>
      <label for='specialChargebackIndicator'>Special Chargeback Indicator</label>
      <select id='specialChargebackIndicator' name='specialChargebackIndicator' class='form-control'>
        <option value=' '>Field not applicable</option>
        <option value='P'>Partial amount chargeback</option>
      </select>
    </div>
    <input type='hidden' class='form-control' id='interfaceTraceNumber' name='interfaceTraceNumber' maxlength='6' value='000000'>
    <div class='form-group'>
      <label for='acceptanceTerminalIndicator'>Acceptance Terminal Indicator</label>
      <select id='acceptanceTerminalIndicator' name='acceptanceTerminalIndicator' class='form-control'>
        <option value=' '>N/A</option>
        <option value='1'>Unattended cardholder activated, below-floor-limit transaction</option>
        <option value='2'>Unattended chip and PIN transactions</option>
        <option value='3'>Unattended cardholder activated (VE use only)</option>
        <option value='4'>Remote Indicator</option>
        <option value='5'>Unattended Consumer Device</option>
        <option value='9'>Mobile Acceptance Solution</option>
      </select>
    </div>
    <div class='form-group'>
      <label for='prepaidCardIndicator'>Prepaid Card Indicator</label>
      <select id='prepaidCardIndicator' name='prepaidCardIndicator' class='form-control'>
        <option value=' '>N/A</option>
        <option value='P'>Prepaid card</option>
        <option value='L'>Prepaid Load</option>
      </select>
    </div>
    <div class='form-group'>
      <label for='serviceDevelopmentField'>Service Development Field</label>
      <select id='serviceDevelopmentField' name='serviceDevelopmentField' class='form-control'>
        <option value='0'>Not used</option>
        <option value='1'>Electronic commerce</option>
        <option value='2'>In-flight Commerce</option>
        <option value='5'>Transponder-Initiated</option>
        <option value='6'>Relationship Participant</option>
        <option value='7'>Deferred Billing Indicator</option>
      </select>
    </div>
    <div class='form-group'>
      <label for='avsResponseCode'>AVS Response Code</label>
      <input type='text' class='form-control' id='avsResponseCode' name='avsResponseCode' maxlength='1'>
    </div>
    <div class='form-group'>
      <label for='authSourceCode'>Authorization Source Code</label>
      <input type='text' class='form-control' id='authSourceCode' name='authSourceCode' maxlength='1'>
    </div>
    <div class='form-group'>
      <label for='purchaseIdentifierFormat'>Purchase Identifier Format</label>
      <select id='purchaseIdentifierFormat' name='purchaseIdentifierFormat' class='form-control'>
        <option value='0'>0</option>
        <option value='1'>1</option>
        <option value='3'>3</option>
        <option value='4'>4</option>
        <option value='5'>5</option>
      </select>
    </div>
    <div class='form-group'>
      <label for='accountSelection'>Account Selection</label>
      <select id='accountSelection' name='accountSelection' class='form-control'>
        <option value=' '>N/A</option>
        <option value='0'>Unavailable</option>
        <option value='1'>Savings Account</option>
        <option value='2'>Checking Account</option>
        <option value='3'>Credit Card Account</option>
        <option value='D'>Debit</option>
        <option value='P'>Prepaid</option>
      </select>
    </div>
    <div class='form-group'>
      <label for='installmentPaymentCount'>Installment Payment Count</label>
      <input type='text' class='form-control' id='installmentPaymentCount' name='installmentPaymentCount' maxlength='2'>
    </div>
    <div class='form-group'>
      <label for='purchaseIdentifier'>Purchase Identifier</label>
      <input type='text' class='form-control' id='purchaseIdentifier' name='purchaseIdentifier' maxlength='25'>
    </div>
    <div class='form-group'>
      <label for='cashback'>Cashback</label>
      <input type='text' class='form-control' id='cashback' name='cashback' maxlength='9'>
    </div>
    <div class='form-group'>
      <label for='chipConditionCode'>Chip Condition Code</label>
      <select id='chipConditionCode' name='chipConditionCode' class='form-control'>
        <option value=' '>Space</option>
        <option value='0'>0</option>
        <option value='1'>1</option>
        <option value='2'>2</option>
      </select>
    </div>
    <div class='form-group'>
      <label for='posEnvironment'>POS Environment</label>
      <select id='posEnvironment' name='posEnvironment' class='form-control'>
        <option value=' '>Default</option>
        <option value='R'>Recurring</option>
        <option value='I'>Installment</option>
      </select>
    </div>
    <br>
    <button type='button' id='deleteTCR0' class='btn btn-danger'>Delete</button>
  </form>
</div>