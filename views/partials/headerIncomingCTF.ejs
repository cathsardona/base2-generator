        <label for='securityCode'>Security Code</label>
        <input type='text' class='form-control' id='securityCode' name='securityCode' maxlength='8'>
      </div>
    </div>
    <div class='col-sm-4'>
      <input type='hidden' class='form-control' id='reserved4' name='reserved4' maxlength='6' value='      '>
      <div class='form-group'>
        <label for='incomingFileID'>Incoming File ID</label>
        <input type='text' class='form-control' id='incomingFileID' name='incomingFileID' maxlength='3'>
      </div>
      <input type='hidden' class='form-control' id='reserved5' name='reserved5' maxlength='89' value='                                                                                         '>
      <br>
      <button type='button' id='delete' class='btn btn-danger'>Delete</button>
    </div>
