        <label for='processingBIN'>Processing BIN</label>
        <input type='text' class='form-control' id='processingBIN' name='processingBIN' maxlength='6'>
      </div>
      <div class='form-group'>
        <label for='processingDate'>Processing Date (YYDDD)</label>
        <input type='text' class='form-control' id='processingDate' name='processingDate' maxlength='5'>
